display "--- [ COUNTER ] ---" lf;
$counter = 0;
while $counter < 10
{
    display $counter lf;
    $counter = $counter + 1;
}

display lf "--- [ FIBONACCI ] ---" lf;
$a = 0;
$b = 1;
display $a lf;
display $b lf;
$n = 0;
while $n < 10
{
    $swap = $a;
    $a = $b;
    $b = $b + $swap;
    display $b lf;
    $n = $n + 1;
}

display lf "--- [ DISAN COUNT ] ---" lf;
$n = 18;
while $n >= 0
{
    if $n % 2 == 0 {
        display $n " es par!" lf;
    }
    $n = $n - 1;
}

